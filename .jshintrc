{
  "bitwise": true,
  "curly": true,
  "eqeqeq": true,
  "es3": true,
  "forin": true,
  "freeze": true,
  "futurehostile": true,
  "predef": [
    "ThreeTwist",
    "THREE",
    "TWEEN",
    "_",
    "R", "O", "Y", "G", "B", "W", "COLORLESS", // Defined in colors.js
    "console" // For logging. Maybe add a release build that removes logging?
  ],
  "latedef": "nofunc",
  "maxcomplexity": 24,
  "maxdepth": 8,
  "maxerr": 200,
  "maxparams": 8,
  "maxstatements": 100,
  "noarg": true,
  "nocomma": true,
  "nonbsp": true,
  "nonew": true,
  "undef": true,
  "unused": true,
  "browser": true,
  "elision": true
}
